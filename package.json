    "solidity-coverage": "^0.5.4",
    "truffle": "^4.1.11"
  },
  "dependencies": {
    "aws-sdk": "^2.255.1",
    "bluebird": "^3.5.1",
    "crypto-js": "^3.1.9-1",
    "eth-gas-reporter": "^0.1.7",
    "lodash": "^4.17.10",
